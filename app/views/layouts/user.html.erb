<!DOCTYPE html>
<html>
<head>
  <title>Schedule</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<% if @error %>
	<div id="fail">
		<%= @error %>
	</div>
<% end %>
<% if @correct %>
	<div id="success">
		<%= @correct %>
	</div>
<% end %>

<div id="page">
	<div id="head"><%= @say %> </div>
<%= yield %>
</div>

</body>
</html>
